            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <Label fx:id="confirmPasswordLable" alignment="CENTER" layoutX="58.0" layoutY="326.0" prefHeight="19.0" prefWidth="489.0" textFill="#f7270c">
